<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.smokingappdraft" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SmokingAppDraft"
        android:usesCleartextTraffic="true"
        tools:targetApi="31" >
        <activity
            android:name=".CommunityForumActivity"
            android:label="Community Forum"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AdminCommunitySettingsActivity"
            android:label="Community Setting"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".PaymentActivity"
            android:label="Payment Activity"
            android:parentActivityName=".Subscription" />
        <activity
            android:name=".Subscription"
            android:label="Subscription"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AdminNotificationSettingsActivity"
            android:label="Notification Setting"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>